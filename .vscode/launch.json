    "version": "0.2.0",
    "configurations": [
        {
            "name": "test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                // "-u",
                // "test.py",
                "./DATASETS/cifar.python",
                "cifar10",
                "--ngpu",
                "2",
                "--load",
                "./snapshots/cifar10_model.pytorch",
                "--test_bs",
                "128",
                // "--gpu_id_list=3,5"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "train",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                // "-u",
                // "train.py",
                "./DATASETS/cifar.python",
                "cifar10",
                "-s",
                "./snapshots",
                "--log",
                "./logs",
                "--ngpu",
                "4",
                "--learning_rate",
                "0.05",
                "-b",
                "128",
                "--gpu_id_list=3,4,5,6"
            ],
            "console": "integratedTerminal"
        }
    ]
